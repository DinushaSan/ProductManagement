#!groovy
pipeline {
agent {
    label 'docker' 
  }
    agent {
        docker {
			
            image 'maven:4.0.0'
            args '--privileged'
        }
    }
    stages {
        stage('Debug Docker Access') {
            steps {
                sh 'whoami && groups && docker ps'
            }
        }
        stage('Maven Install') {
            steps {
                sh 'mvn clean install'
            }
        }
    }
}
